
#  BlitzHere Status Server configuration

service-name        Status-Server
service-prefix      "/status"
service-file-path   Status
service-port        10006
server-url          "https://blitzhere.com"

testing-enabled     true

database-url        "postgres://blitzlabs:blitzlabs@localhost:5432/blitzlabs"

log-level           LogLevelAll
log-filename        "~/log/Status-Server.log"

http-access-log     "/var/log/nginx/access.log"
